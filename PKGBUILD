pkgname=cups-filter-oopstops
pkgver=1.0.0
pkgrel=1
pkgdesc="prefilter to sanitize PostScript jobs generated by OpenOffice 2.x"
url="http://stuff.mit.edu/afs/athena/system/i386_deb50/os/usr/lib/cups/filter/oopstops"
license=('GPLv2')
arch=('i686' 'x86_64')
depends=('cups' 'cups-filters')
makedepends=('libtool')
source=(http://stuff.mit.edu/afs/athena/system/i386_deb50/os/usr/lib/cups/filter/oopstops)
md5sums=('8f6060016385aa0d132382a65786c6dd')

package() {
  cd ${srcdir}
  mkdir -p ${pkgdir}/usr/lib/cups/filter
  cp oopstops ${pkgdir}/usr/lib/cups/filter
  chmod +x ${pkgdir}/usr/lib/cups/filter/oopstops
}
